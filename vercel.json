{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/api/generate-bulk",
      "dest": "/api/generate-bulk.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
